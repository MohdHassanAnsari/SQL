DELIMITER $

CREATE PROCEDURE GET_INVOICES_BY_CLIENT_ID(CLIENT_ID 
INT) BEGIN 
	SELECT *
	FROM sql_invoicing.invoices i
	WHERE i.client_id = client_id;
END$ 

DELIMITER ;
-- SQLBook: Code
CALL GET_INVOICES_BY_CLIENT_ID(2) 